{
    "name": "cone-lang",
    "displayName": "Cone",
    "description": "Syntax highlighting for Cone Language",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/pegicon.png",
    "repository": "https://github.com/Ivo-Balbaert/vscode-cone",
    "publisher": "Ivo-Balbaert",
    "contributes": {
        "languages": [
            {
                "id": "Cone",
                "aliases": [
                    "Cone",
                    "cone"
                ],
                "extensions": [
                    ".cone"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Cone",
                "scopeName": "source.cone",
                "path": "./syntaxes/cone.tmLanguage.json"
            }
        ]
    }
}